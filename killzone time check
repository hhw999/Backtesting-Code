from datetime import datetime, time

def check_time_range(timestamp_str):
    # Convert the timestamp string to a datetime object
    timestamp = datetime.strptime(timestamp_str, "%Y-%m-%d %H:%M:%S")

    # Define time ranges
    time_range_1_start = time(10, 0)
    time_range_1_end = time(12, 30)
    time_range_2_start = time(13, 0)
    time_range_2_end = time(15, 30)

    # Extract the time from the timestamp
    current_time = timestamp.time()

    # Check if the time is within either of the defined ranges
    if (time_range_1_start <= current_time <= time_range_1_end) or \
       (time_range_2_start <= current_time <= time_range_2_end):
        return True
    else:
        return False

# Example usage
data_timestamp = "2023-08-18 11:30:00"
is_within_desired_range = check_time_range(data_timestamp)
print(is_within_desired_range)
